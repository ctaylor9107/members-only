<h1>New Post</h2>


<%= form_with model: @post do |form| %>
    <div>
        <%= form.label :title %><br>
        <%= form.text_field :title %>
    </div>

    <div>
        <%= form.label :body %><br>
        <%= form.text_field :body %><br>
    </div>

    <div>
        <%= form.submit %>
    </div>
<% end %>